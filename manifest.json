{
    "manifest_version": 3,
    "name": "LinkedIn PuzzleBot",
    "version": "0.1.0",
    "description": "Solve LinkedIn puzzles in-page.",
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "https://www.linkedin.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "LinkedIn PuzzleBot"
    }
}